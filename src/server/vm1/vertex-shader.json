{ "is": [ "3d", "vertex", "shader", "string", "list" ],
  "title": "Dynamic Phong Vertex Shader",
  "list": [
    "uniform mat4 mvpm, mvvm;",
    "attribute vec4 pos;",
    "attribute vec2 tex;",
    "attribute vec3 nor;",
    "varying vec3 mvvp;",
    "varying vec2 texturePt;",
    "varying vec3 mvvn;",
    "void main(){",
      "texturePt = tex;",
      "mvvp = vec3(mvvm*pos);",
      "mvvn = vec3(mvvm*vec4(nor,0.0));",
      "gl_Position = mvpm*pos;",
    "}"
  ]
}
