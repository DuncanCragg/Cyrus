{ "is": [ "3d", "fragment", "shader", "string", "list" ],
  "title": "Dynamic Phong Fragment Shader",
  "list": [
    "precision mediump float;",
    "uniform sampler2D texture0;",
    "varying vec4 colour;",
    "varying vec2 texturePt;",
    "void main(){",
      "gl_FragColor=colour*texture2D(texture0,texturePt);",
    "}"
  ]
}
