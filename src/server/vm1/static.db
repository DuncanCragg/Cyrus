{ UID: uid-3c3a-7576-a61b-ed87
  is: ( fibonacci rule )
  when: "running z is sum of x and y"
  y: <lt($:config:max)>
  z: <>sum($:x $:y)
  o: { n: <>$:z }
  seq: <>has($:o)
  ratio: <>div($:y $:x)
}

{ UID: uid-61fd-78a2-60b6-4c27
  is: ( fibonacci rule )
  when: "z calculated shift back"
  z: <sum($:x $:y)>
  x: <>$:y
  y: <>$:z
}

