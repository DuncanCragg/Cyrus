{ UID: uid-83a6-53ef-9f44-43bd
  is:  3d cuboid editable rule
  when: user present, set target coords to just in front
  place: {
    present: > 0
    sub-objects: { object: {  is: user } }
  }
  target-coords: => ( @=place:sub-objects:coords select 0 ) v+v ( 0 0 -3 )
}

{ UID: uid-101b-2c33-9fd6-96a1
  is:  3d cuboid editable rule
  when: time passes, set coords to half way to target
  time: { timestamp: > @next-time }
  next-time: => @time:timestamp + 1000
  coords: => ( @. v+v @target-coords ) v/s 2
}

