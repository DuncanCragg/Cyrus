{ "%uid": "uid-ff5d-1ef4-cfa5-5f92",
  "is": [ "3d", "vertex", "shader", "string", "list" ],
  "title": "Phong Vertex Shader",
  "list": [
    "uniform mat4 mvpm, norm;",
    "uniform vec4 lightPos;",
    "attribute vec4 pos;",
    "attribute vec3 nor;",
    "attribute vec2 tex;",
    "varying vec3 NN;",
    "varying vec3 LL;",
    "varying vec3 VV;",
    "varying vec2 TT;",
    "void main() {",
      "vec4 glp = mvpm * pos;",
      "NN = vec3(norm * vec4(nor, 1.0));",
      "LL = -vec3(mvpm * lightPos);",
      "VV = -glp.xyz;",
      "TT = tex;",
      "gl_Position = glp;",
    "}"
  ]
}

{ "%uid": "uid-6e92-c60d-d16a-6b35",
  "is": [ "3d", "fragment", "shader", "string", "list" ],
  "title": "Phong Fragment Shader",
  "list": [
    "precision mediump float;",
    "uniform vec4 ambient, diffuse, specular;",
    "uniform vec4 lightPos;",
    "uniform float shininess;",
    "uniform sampler2D texture0;",
    "varying vec3 NN;",
    "varying vec3 VV;",
    "varying vec3 LL;",
    "varying vec2 TT;",
    "void main(){",
      "vec3 L=normalize(LL);",
      "vec3 N=normalize(NN);",
      "vec3 V=normalize(VV);",
      "gl_FragColor=ambient*texture2D(texture0, TT)+diffuse*max(dot(N,normalize(-vec3(lightPos))),0.0)+specular*pow(max(dot(reflect(L,N), V), 0.0), shininess);",
    "}"
  ]
}

