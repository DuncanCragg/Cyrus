{ is: 3d fragment shader string list editable
  title: "Dynamic Phong Fragment Shader"
  list:
    "precision mediump float;"
    "uniform vec3 lightPos;"
    "uniform vec3 lightCol;"
    "uniform sampler2D texture0;"
    "varying vec3 mvvp;"
    "varying vec2 texturePt;"
    "varying vec3 mvvn;"
    "void main(){"
      "float lgtd=length(lightPos-mvvp);"
      "vec3 lgtv=normalize(lightPos-mvvp);"
      "float dffus=max(dot(mvvn,lgtv),0.1)*(1.0/(1.0+(0.25*lgtd*lgtd)));"
      "gl_FragColor=vec4(lightCol,1.0)*(0.30+0.85*dffus)*texture2D(texture0,texturePt);"
    " }"
}

