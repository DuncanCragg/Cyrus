{   "%uid": "uid-424d-01c5-ee08-2ec9",
    "is": [ "fx", "dealer", "rule" ],
    "when": "FX order comes in, start building ticket. 1",
    "%alerted": { "is": "order", "ticket": "<#>" },
    "ticketfor": "<#>%alerted",
    "ticket": "<#>{}"
}

{   "%uid": "uid-e772-4f4f-f981-0cea",
    "is": [ "fx", "dealer", "rule" ],
    "when": "FX order comes in, spawn and add ticket to tickets list. 2",
    "ticket": {  "%rules": "<#>list(uid-05cf-8cb9-7b52-db80, uid-bce3-b010-e869-f26e, uid-1b6c-16b6-64aa-8d48, uid-a36b-54ea-7dd6-2626, uid-913e-f82c-7198-165b, uid-4f84-6624-1781-3377)",
                 "is": "<#>list(fx, ticket)",
                 "order": "<#>$:ticketfor",
                 "ask": "<#>81.9",
                 "status": "<#>waiting",
                 "%uid": "<#>new"
    },
    "tickets": "<>has($:ticket)"
}

{   "%uid": "uid-05cf-8cb9-7b52-db80",
    "is": [ "fx", "ticket", "rule" ],
    "when": "new ticket, set params and notify order. 3",
    "params": "<#>$:order:params",
    "%notifying": "<>has($:order)"
}

{   "%uid": "uid-bce3-b010-e869-f26e",
    "is": [ "fx", "ticket", "rule" ],
    "when": "waiting, if order params change, update ticket params. 4",
    "status": "waiting",
    "params": "<{}>$:order:params"
}

{   "%uid": "uid-4f84-6624-1781-3377",
    "is": [ "fx", "ticket", "rule" ],
    "when": "ask goes below price of ticket, set to filled. 4.5",
    "ask": "<lt($:params:price)>",
    "status": "<waiting>filled"
}

{   "%uid": "uid-1b6c-16b6-64aa-8d48",
    "is": [ "fx", "ticket", "rule" ],
    "when": "order differs once ticket filled, flag with not-as-ordered. 5",
    "params": "<!$:order:params>",
    "status": "<filled>has(not-as-ordered)"
}

{   "%uid": "uid-a36b-54ea-7dd6-2626",
    "is": [ "fx", "ticket", "rule" ],
    "when": "order matches once ticket filled, remove not-as-ordered. 6",
    "params": "<$:order:params>",
    "status": "<filled>hasno(not-as-ordered)"
}

{   "%uid": "uid-913e-f82c-7198-165b",
    "is": [ "fx", "ticket", "rule" ],
    "when": "correct payment arrives, set status to paid. 7",
    "order": { "payment": { "amount": "<prod($:ask, $:params:investment)>" } },
    "status": "<filled>paid",
    "payment": "<#>$:order:payment"
}

