    "when": "you see the order filled, simulate a race by dropping the price. 4",
    "ticket": { "status": "filled" },
    "payment": "<#>",
    "params": { "price": "<81.7>81.5" }
}

{   "%uid": "uid-4e30-2365-3015-e181",
