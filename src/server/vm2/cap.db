  "is": [ "gui", "editable" ],
  "title": "CAP Payments Demo",
  "view": { "logo": "http://www.shaunthesheep.be/shaunthesheep/foto/500/thefarmer-1.jpg",
            "text": "Welcome to the Farmer Support Agency!",
            "start": { "input": "button", "label": "Apply for farming support" }
