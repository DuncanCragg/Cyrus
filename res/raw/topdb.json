    "%class": "netmash.forest.Fjord",
    "%rules": [ "uid-58cb-cc31-1769-c2c4" ],
    "is": [ "gui" ],
    "title": "On-board Demo",
    "view": {
        "style": { "is": "style", "direction": "vertical" },
        "#stuff": [
            { "is": "style", "direction":"vertical" },
            { "#a": "?[A /boolean/ ]?" },
            { "#b": "?[B /boolean/ ]?" },
            { "style": { "is": "style", "direction":"horizontal", "proportions":"98%" },
              "#t": "?[new title /string/]?" },
         ],
        "#result":  { "style": { "is": "style", "direction":"horizontal",
                                 "proportions":"50%", "colours":"lightturquoise" },
                      "#c": "..." }
    }
}

