{ "%uid": "uid-1b56-01ce-b820-ee90",
  "is": [ "event", "rule" ],
  "when": "articles seen, get titles, dates and make into event",
  "coord": { "articles-a": {} },
  "title": "<>$:coord:articles-a:title",
